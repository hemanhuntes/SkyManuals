{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": ["<rootDir>/test/setup.ts"],
  "testTimeout": 60000,
  "moduleNameMapping": {
    "^@skymanuals/types$": "<rootDir>/../../packages/types/src/index.ts"
  }
}
